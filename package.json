{
  "name": "@2bad/ryanair",
  "version": "1.0.0",
  "description": "Unofficial typescript client for the Ryanair API that allows you to easily retrieve information about airports, flights and prices.",
  "keywords": [
    "api-client",
    "api",
    "client",
    "rest-client",
    "rest",
    "ryanair",
    "typescript"
  ],
  "homepage": "https://github.com/2BAD/ryanair#readme",
  "bugs": {
    "url": "https://github.com/2BAD/ryanair/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/2BAD/ryanair.git"
  },
  "license": "MIT",
  "author": "2BAD (https://github.com/2BAD)",
  "contributors": [
    {
      "name": "Jason Hyde",
      "email": "github@2bad.me",
      "url": "https://github.com/mrhyde"
    }
  ],
  "type": "module",
  "exports": "./build/index.js",
  "typings": "./build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "clean": "rimraf build",
    "format:check": "prettier --check source/**/*.ts",
    "format:fix": "prettier --write source/**/*.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "run-s \"lint -- --fix\"",
    "prepublishOnly": "run-s clean build",
    "preversion": "run-s clean lint test build",
    "test": "vitest run",
    "test:integration": "vitest run integration --coverage"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "dependencies": {
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@2bad/tsconfig": "1.5.1",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "@vitest/coverage-c8": "0.30.1",
    "dotenv": "16.0.3",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "nock": "13.3.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.7",
    "rimraf": "5.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "vitest": "0.30.1"
  },
  "engines": {
    "node": ">=18"
  },
  "volta": {
    "node": "19.8.1"
  }
}
